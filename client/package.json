{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:old": "tsc && vite build && npm run copy-builded",
    "copy-builded": "export RUN_OS_WINBASH_IS_LINUX=true || RUN_OS_WINBASH_IS_LINUX=true && run-script-os",
    "copy-builded:win32": "xcopy dist\\content.* ..\\dist\\bundled /i /Y & xcopy dist\\images ..\\extension\\bundled\\images /i /Y",
    "copy-builded:nix": "mkdir -p ../extension/bundled && cp -R dist/content.* ../extension/bundled & mkdir -p ../extension/bundled/images && cp -R dist/images/. ../extension/bundled/images/",
    "preview": "vite preview"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-portal": "^1.0.4",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/chrome": "^0.0.260",
    "@types/node": "^20.9.3",
    "@yaredfall/tw-grid-auto-fill": "^0.1.1",
    "clsx": "^2.0.0",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.7",
    "ky": "^1.1.3",
    "plural-ru": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.19.0",
    "react-transition-group": "^4.4.5",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@shrutibalasa/tailwind-grid-auto-fit": "^1.1.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.33",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animated": "^1.0.1",
    "terser": "^5.27.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
